    <ThingDef ParentName="SK_ResourceBase">
		<defName>Neutroamine</defName>
		<label>neutroamine</label>
		<description>A synthetic precursor chemical made from an artificially engineered plant. Useless on its own, many drugs require neutroamine as an ingredient.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Neutroamine</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.90</drawSize>
		</graphicData>
		<stackLimit>400</stackLimit>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<MarketValue>10</MarketValue>
			<Flammability>1.0</Flammability>
			<WorkToMake>700</WorkToMake>
			<Mass>0.02</Mass>
			<DeteriorationRate>1.0</DeteriorationRate> 
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<costList>
			<NeutroPetals>6</NeutroPetals>
            <SyntheticAmmonia>1</SyntheticAmmonia>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			<workSkill>Medicine</workSkill>
			<recipeUsers>
				<li>DrugLab</li>
			</recipeUsers>
			<skillRequirements>
				<Medicine>6</Medicine>
				<Crafting>3</Crafting>
			</skillRequirements>	
		    <researchPrerequisite>Drugs_C2</researchPrerequisite>
		</recipeMaker>
	</ThingDef>



